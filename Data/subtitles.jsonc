	"SFX\\Room\\Intro\\Guard\\Ulgrin\\EscortKill1.ogg": [
		{
			"text": "Гаразд, добре. Хай так і буде. Тоді ми просто знайдемо когось іншого.",
			"color": "guard"
		}
