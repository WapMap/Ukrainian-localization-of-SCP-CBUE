	"SFX\\Room\\Intro\\Guard\\Conversation3a.ogg": [
		{
			"text": "В тебе ж немає дівчини, так?",
			"color": "guard"
		},
		{
			"text": "Ну, я точно не читач думок. Інакше я б був замкнений тут. Тож, так, я б сказав, що це досить очевидно.",
			"delay": 3.3,
			"color": "guard"
