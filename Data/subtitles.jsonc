	/* GUARD CONVERSATION */
	"SFX\\Room\\Intro\\Guard\\Conversation0a.ogg": [
		{
			"text": "Так, кхм... як справи?",
			"color": "guard"
		},
		{
			"text": "Ну так, з ким ти думаєш, я розмовляю? З цим хлопцем, з обличчям яке можна вдарити? Звісно я розмовляю з тобою.",
			"delay": 5.3,
			"color": "guard"
		},
		{
			"text": "Ну так. Це твій перший робочий день тут.",
			"delay": 15.4,
			"color": "guard"
		},
		{
			"text": "Справді? Ого. Це дивно.",
			"delay": 23.65,
			"color": "guard"
		}
	],
	"SFX\\Room\\Intro\\Guard\\Conversation0b.ogg": [
		{
			"text": "Е-ем... т-ти розмовляєш зі мною?",
			"delay": 3.2,
			"color": "guard"
		},
		{
			"text": "Оу! Я просто трохи здивований. Здається, ти вперше зі мною розмовляєш.",
			"delay": 10.33,
			"color": "guard"
		},
		{
			"text": "Ем... насправді ми працюємо разом вже близько п'яти місяців.",
			"delay": 18.95,
			"color": "guard"
		}
	],
	"SFX\\Room\\Intro\\Guard\\Conversation1a.ogg": [
		{
			"text": "Ем... то ти бачив якісь хороші фільми останнім часом?",
